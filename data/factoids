<voteresults>
action: { return "/say No vote started yet." if not defined $question; return "/say No votes for \"$question\" yet." if not keys %votes; %count = (); map { $count{$_}++ } values %votes; $result = "Poll results for \"$question\": "; $comma = ""; map { $result .= "$comma$_: $count{$_}"; $comma = ', '; } sort { $count{$b} <=> $count{$a} } keys %count; "/say $result" }
created_on: 1503557862.46898
edited_by: pragma-!~chaos@unaffiliated/pragmatic-chaos
edited_on: 1503605067.10534
enabled: 1
interpolate: 0
last_referenced_in: ##c-offtopic
last_referenced_on: 1504179114.90059
locked: 1
owner: pragma-!~chaos@unaffiliated/pragmatic-chaos
persist-key: vote-poll
